{
	"C_Cpp.vcpkg.enabled": true,
	"C_Cpp.default.cppStandard": "",
	"C_Cpp.default.cStandard": "",
	"C_Cpp.default.windowsSdkVersion": "10.0.19041.0",
	"C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
	"C_Cpp.configurationWarnings": "Enabled",
	"C_Cpp.loggingLevel": "Debug",
	"C_Cpp.default.defines": [
		"UNICODE",
		"_UNICODE"
	],
	"C_Cpp.default.mergeConfigurations": true,
	"C_Cpp.default.browse.path": [
		"${default}",
		"E:/StoneyDSP/**",
		"${workspaceFolder}/**",
		"${vcpkgRoot}/x64-windows/include",
		"${vcpkgRoot}/x86-windows/include"
	],
	"C_Cpp.default.includePath": [
		"${default}",
		"E:/StoneyDSP/**",
		"${workspaceFolder}/**",
		"${vcpkgRoot}/x64-windows/include",
		"${vcpkgRoot}/x86-windows/include"
	],
	"C_Cpp.files.exclude": {
		"**/.vscode": true,
		"**/.vs": true,
		"**/html": true,
		"**/css": true,
		"**/docs": true
	},
	"cmake.useCMakePresets": "auto",
	"cmake.generator": "Ninja",
	"cmake.configureOnOpen": true,
	"cmake.configureOnEdit": true,
	"cmake.skipConfigureIfCachePresent": false,
	"cmake.saveBeforeBuild": true,
	"cmake.buildBeforeRun": true,
	"cmake.buildTask": false,
	"cmake.sourceDirectory": "${workspaceFolder}",
	"cmake.clearOutputBeforeBuild": true,
	"cmake.cmakeCommunicationMode": "automatic",
	"cmake.statusbar.visibility": "default",
	"cmake.mingwSearchDirs": [
		"${env:SystemDrive}/MinGW",
		"${env:SystemDrive}/msys64"
	],
	"cmake.debugConfig": {
		"cwd": "${workspaceFolder}",
		"symbolSearchPath": "${workspaceFolder}\\out\\build\\x64\\",
		"miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
		"MIMode": "gdb",
		"console": "newExternalWindow",
		"stopAtEntry": false,
		"logging": {
			"engineLogging": false,
			"exceptions": true,
			"moduleLoad": true,
			"trace": false,
			"traceResponse": false,
			"programOutput": true
		}
	},
	"editor.insertSpaces": false,
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"files.associations": {
		"atomic": "cpp",
		"bit": "cpp",
		"cctype": "cpp",
		"clocale": "cpp",
		"cmath": "cpp",
		"compare": "cpp",
		"complex": "cpp",
		"concepts": "cpp",
		"cstddef": "cpp",
		"cstdint": "cpp",
		"cstdio": "cpp",
		"cstdlib": "cpp",
		"cstring": "cpp",
		"ctime": "cpp",
		"cwchar": "cpp",
		"exception": "cpp",
		"initializer_list": "cpp",
		"ios": "cpp",
		"iosfwd": "cpp",
		"istream": "cpp",
		"iterator": "cpp",
		"limits": "cpp",
		"memory": "cpp",
		"new": "cpp",
		"ostream": "cpp",
		"sstream": "cpp",
		"stdexcept": "cpp",
		"streambuf": "cpp",
		"string": "cpp",
		"system_error": "cpp",
		"tuple": "cpp",
		"type_traits": "cpp",
		"typeinfo": "cpp",
		"utility": "cpp",
		"vector": "cpp",
		"xfacet": "cpp",
		"xiosbase": "cpp",
		"xlocale": "cpp",
		"xlocinfo": "cpp",
		"xlocnum": "cpp",
		"xmemory": "cpp",
		"xstddef": "cpp",
		"xstring": "cpp",
		"xtr1common": "cpp",
		"xutility": "cpp"
	},
	"json.schemas": [
		{
			"fileMatch": [
				"/vcpkg.json"
			],
			"url": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json"
		}
	],
	"git.enabled": true,
	"git.autofetch": "all",
	"git.detectSubmodules": true,
	"git.detectSubmodulesLimit": 10,
	"git.ignoreSubmodules": false,
	"git.requireGitUserConfig": true,
	"git.pullTags": true,
	"git.scanRepositories": [
		"E:/StoneyDSP/"
	],
	"git.showActionButton": {
		"commit": true,
		"publish": true,
		"sync": true
	},
	"git.fetchOnPull": true,
	"git.showProgress": true,
	"github.gitAuthentication": true,
	"githubPullRequests.queries": [
		{
			"label": "Waiting For My Review",
			"query": "is:open review-requested:${user}"
		},
		{
			"label": "Assigned To Me",
			"query": "is:open assignee:${user}"
		},
		{
			"label": "Created By Me",
			"query": "is:open author:${user}"
		}
	],
	"githubIssues.queries": [
		{
			"label": "My Issues",
			"query": "default"
		},
		{
			"label": "Created Issues",
			"query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
		},
		{
			"label": "Recent Issues",
			"query": "state:open repo:${owner}/${repository} sort:updated-desc"
		}
	]
}
